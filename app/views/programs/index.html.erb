<h1>Programs</h1>

<table>
  <thead>
  <tr>
    <th>Event name</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @programs.each do |program| %>
      <tr>
        <td><%= program.Event_Name %></td>
        <td><%= link_to 'Show', program,:id=> 'program.id' %></td>
        <td><%= button_to( "Donate", new_mdo_path, :method => :get )%></td>
        <% if user_signed_in? %> <td><%= link_to 'Edit', edit_program_path(program) %></td><% end %>
        <% if user_signed_in? && current_user.email == 'admin@ait.asia' %> <td><%= link_to 'Delete Event', program, method: :delete, data: { confirm: 'Are you sure?' } %></td><% end %>
      </tr>
  <% end %>
  </tbody>
</table>

<br>


<% if user_signed_in?  %><%= button_to( "Add New Event", new_program_path, :method => :get )%><% end %>

